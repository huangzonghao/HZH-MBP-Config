SYSTEM_VER="mac"
RC_FILE="~/.bash_profile"

source ~/dotfiles/bashrc_common
if [ -e ~/dotfiles/local_config/bashrc_local ]
then
    source ~/dotfiles/local_config/bashrc_local
fi

# ls color for the black background
export LSCOLORS=gxBxhxDxfxhxhxhxhxcxcx

# matlab cmd mode
alias matlab="/Applications/MATLAB_R2014a.app/bin/matlab -nodesktop"
alias mt="cd ~/TEMP/matlab && /Applications/MATLAB_R2014a.app/bin/matlab -nodesktop"
